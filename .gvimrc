"----------
" 基本的な設定
"----------
set showtabline=2
set transparency=6
set imdisable
set guioptions-=T
set antialias
set tabstop=4
set number
set nobackup
set visualbell t_vb=
set nowrapscan
set columns=163
set lines=75
set guifontwide=Ricty:h15
set guifont=Ricty:h15

colorscheme solarized
let g:solarized_termtrans=0
let g:solarized_degrade=0
let g:solarized_menu=0

let g:tex_conceal='adsmg'
set conceallevel=0
